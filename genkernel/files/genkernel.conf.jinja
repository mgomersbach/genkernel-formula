# Do not modify, this file is managed from Salt

{% set config_values = {} %}
{% for item in salt['pillar.get']('genkernel:config', {}) %}
  {% for key, value in item.items() %}
    {% set _ = config_values.update({key: value}) %}
  {% endfor %}
{% endfor %}

{% for key, value in config_values.items() %}
{{ key.upper() }}="{{ value }}"
{% endfor %}
